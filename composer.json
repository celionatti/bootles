{
    "name": "celionatti/bootles",
    "description": "Bootles Slim MVC Framework",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bootles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "authors": [
        {
            "name": "celionatti",
            "email": "amisuusman@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-pdo": "*",
        "slim/slim": "^4.11",
        "twig/twig": "^3.6",
        "vlucas/phpdotenv": "^5.5",
        "vlucas/valitron": "^1.4",
        "twig/intl-extra": "^3.6",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/twig-bridge": "^6.3",
        "symfony/console": "^6.3",
        "symfony/cache": "^6.3",
        "slim/twig-view": "^3.3",
        "slim/psr7": "^1.6",
        "php-di/php-di": "^7.0",
        "laminas/laminas-diactoros": "^3.0",
        "doctrine/migrations": "^3.6",
        "doctrine/orm": "^2.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
